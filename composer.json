{
    "name": "jomweb/billplz",
    "description": "PHP Agnostic library for working with BillPlz API",
    "type": "library",
    "keyword": ["Billplz", "Payment Gateway"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Billplz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Billplz\\TestCase\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "jomweb/ringgit": "^1.0.5",
        "laravie/codex": "^3.0",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "php-http/curl-client": "^1.7",
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
