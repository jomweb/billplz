{
    "name": "jomweb/billplz",
    "description": "PHP Agnostic library for working with BillPlz API",
    "type": "library",
    "keyword": ["Billplz", "Payment Gateway"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Billplz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Billplz\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "jomweb/ringgit": "^2.3.1",
        "laravie/codex": "^5.3",
        "laravie/codex-filter": "^1.2",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "php-http/guzzle7-adapter": "^0.1",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
